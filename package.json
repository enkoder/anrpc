{
  "name": "beanstalk",
  "description": "https://netrunner-beanstalk.net",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "pnpm --stream exec eslint",
    "build": "pnpm --stream -r run build",
    "start": "pnpm --stream -r run start",
    "start-api": "pnpm --filter api run start",
    "start-app": "pnpm --filter app run start",
    "start-storybook": "pnpm --filter app run storybook",
    "start-tailwind": "pnpm --filter app exec tailwindcss -i ./src/input.css -o ./src/output.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rspack/cli": "0.3.10",
    "@rspack/core": "0.3.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^3.1.0",
    "wrangler": "^3.18.0"
  }
}
