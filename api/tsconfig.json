{
  "include": ["src/*"],
  "exclude": ["**/node_modules/*"],
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "types": ["@cloudflare/workers-types"],
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCaseInSwitch": true
  }
}
