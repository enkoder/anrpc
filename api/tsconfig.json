{
  "extends": "../tsconfig.json",
  "include": [
    "src/*"
  ],
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "removeComments": true,
    "inlineSourceMap": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "outDir": "../dist",
    "lib": ["ES5"],
    "strict": false,
    "types": ["@cloudflare/workers-types", "jest", "@types/node"]
  }
}
